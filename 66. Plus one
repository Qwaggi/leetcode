class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        def convert(digits):
            
            s = [str(i) for i in digits]   
            
            res = int("".join(s))
            
            return(res) 
            
            
        converted =(convert(digits)+1)  
        
        answer = [int(i) for i in str(converted)]
        
        return answer 
/* 74ms Runtime *\
